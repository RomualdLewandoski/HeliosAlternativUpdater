{% extends 'base.html.twig' %}

{% block title %}FFile{% endblock %}

{% block body %}
    <h1>FFile</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ f_file.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ f_file.name }}</td>
            </tr>
            <tr>
                <th>Path</th>
                <td>{{ f_file.path }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ f_file.url }}</td>
            </tr>
            <tr>
                <th>Sha</th>
                <td>{{ f_file.sha }}</td>
            </tr>
            <tr>
                <th>Size</th>
                <td>{{ f_file.size }}</td>
            </tr>
            <tr>
                <th>IsUpdate</th>
                <td>{{ f_file.isUpdate ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('config_f_file_index') }}">back to list</a>

    <a href="{{ path('config_f_file_edit', {'id': f_file.id}) }}">edit</a>

    {{ include('config_f_file/_delete_form.html.twig') }}
{% endblock %}
